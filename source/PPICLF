c Computational particles
      REAL  PPICLF_Y     (PPICLF_LRS ,PPICLF_LPART)  ! Solution
     >     ,PPICLF_YDOT  (PPICLF_LRS ,PPICLF_LPART)  ! Total solution RHS
     >     ,PPICLF_YDOTC (PPICLF_LRS ,PPICLF_LPART)  ! Coupled solution RHS
     >     ,PPICLF_RPROP (PPICLF_LRP ,PPICLF_LPART)  ! Real particle properties
     >     ,PPICLF_RPROP2(PPICLF_LRP2,PPICLF_LPART)  ! Secondary real particle properties
      COMMON /PPICLF_COMP_PART_R/ PPICLF_Y
     >                           ,PPICLF_YDOT
     >                           ,PPICLF_YDOTC
     >                           ,PPICLF_RPROP
     >                           ,PPICLF_RPROP2

c Previous time step solutions, may grow later
      COMMON /PPICLF_SLN_PREV/  PPICLF_Y1
      REAL  PPICLF_Y1(PPICLF_LRS*PPICLF_LPART)

      COMMON /PPICLF_COMP_PART_I/  PPICLF_IPROP
      INTEGER  PPICLF_IPROP(PPICLF_LIP,PPICLF_LPART) ! Integer particle properties

      COMMON /PPICLF_COMP_NUMBER_I/ PPICLF_NPART
      INTEGER  PPICLF_NPART

c Particle options
      INTEGER PPICLF_NPARAM
      PARAMETER(PPICLF_NPARAM = 100)
      COMMON /PPICLF_PARAM_R/  PPICLF_RPARAM
      REAL  PPICLF_RPARAM(PPICLF_NPARAM)     ! Specified particle parameters
      LOGICAL PPICLF_RESTART
      DATA PPICLF_RESTART /.false./

      INTEGER PPICLF_NDIM
      COMMON /PPICLF_PARAM_I/ PPICLF_NDIM

c Domain size
      COMMON /PPICLF_DOMAIN/ PPICLF_XDRANGE
      REAL  PPICLF_XDRANGE(2,3)

c Findpts and crystal-router handles
      COMMON /PPICLF_CR_FP_HANDLE/ I_FP_HNDL,I_FP_HNDL1, I_CR_HNDL
      INTEGER  I_FP_HNDL,I_FP_HNDL1, I_CR_HNDL

c Communication
      COMMON /PPICLF_COMM_C/ PPICLF_COMM, PPICLF_NP, PPICLF_NID
      INTEGER PPICLF_COMM, PPICLF_NP, PPICLF_NID

c Grid
      COMMON /PPICLF_GRID_INTERFACE/ PPICLF_NELT
      INTEGER PPICLF_NELT

c Ghost particle connection map
      INTEGER              PPICLF_NDXGP,PPICLF_NDYGP,PPICLF_NDZGP
      COMMON /PPICLF_GP_MAP/  PPICLF_NDXGP,PPICLF_NDYGP,PPICLF_NDZGP

      REAL            PPICLF_RDXGP, PPICLF_RDYGP, PPICLF_RDZGP
     >              , PPICLF_D2CHK(3), PPICLF_BINB(6)
      COMMON /PPICLF_GP/ PPICLF_RDXGP, PPICLF_RDYGP, PPICLF_RDZGP
     >                 , PPICLF_D2CHK, PPICLF_BINB

      REAL            PPICLF_BINX(2,PPICLF_BMAX),
     >                PPICLF_BINY(2,PPICLF_BMAX),
     >                PPICLF_BINZ(2,PPICLF_BMAX) 
      COMMON /PPICLF_BIN_R/ PPICLF_BINX, PPICLF_BINY, PPICLF_BINZ

      INTEGER            PPICLF_NBMAX
      COMMON /PPICLF_BIN_I/ PPICLF_NBMAX

      COMMON /PPICLF_GRID_RC/ PPICLF_GRID_FLD, PPICLF_GRID_X
     >                   , PPICLF_GRID_Y, PPICLF_GRID_Z
     >                   , PPICLF_RDX, PPICLF_RDY, PPICLF_RDZ
      REAL*4          PPICLF_GRID_FLD(PPICLF_BX1,PPICLF_BY1,PPICLF_BZ1
     >              , PPICLF_LRP_PRO)
     >              , PPICLF_GRID_X(PPICLF_BX1,PPICLF_BY1,PPICLF_BZ1)
     >              , PPICLF_GRID_Y(PPICLF_BX1,PPICLF_BY1,PPICLF_BZ1)
     >              , PPICLF_GRID_Z(PPICLF_BX1,PPICLF_BY1,PPICLF_BZ1)
      REAL            PPICLF_RDX, PPICLF_RDY, PPICLF_RDZ

      COMMON /PPICLF_GRID_IC/ PPICLF_GRID_I
     >                   , PPICLF_GRID_II
     >                   , PPICLF_GRID_JJ
     >                   , PPICLF_GRID_KK
      INTEGER   PPICLF_GRID_I (PPICLF_BX1, PPICLF_BY1, PPICLF_BZ1)
     >        , PPICLF_GRID_II(PPICLF_BX1, PPICLF_BY1, PPICLF_BZ1)
     >        , PPICLF_GRID_JJ(PPICLF_BX1, PPICLF_BY1, PPICLF_BZ1)
     >        , PPICLF_GRID_KK(PPICLF_BX1, PPICLF_BY1, PPICLF_BZ1)

      COMMON /PPICLF_GRID_PC/ PPICLF_BX, PPICLF_BY, PPICLF_BZ
      INTEGER              PPICLF_BX, PPICLF_BY, PPICLF_BZ


C Ghost particles
      COMMON /PPICLF_GHOST_PART_R/  PPICLF_RPROP_GP
      REAL PPICLF_RPROP_GP(PPICLF_LRP_GP,PPICLF_LPART_GP)  

      COMMON /PPICLF_PRO_FLD_MAP/ PPICLF_CP_MAP
      REAL  PPICLF_CP_MAP(PPICLF_LRP_GP,PPICLF_LPART)

      COMMON /PPICLF_PRO_FLD_CR/ PPICLF_PRO_FLD,PPICLF_PRO_FLDB
     >                          ,PPICLF_XM1B
      REAL PPICLF_PRO_FLD (PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ,PPICLF_LEE
     >                    ,PPICLF_LRP_PRO),
     >     PPICLF_PRO_FLDB(PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ
     >                    ,PPICLF_LRP_PRO,PPICLF_LEE),
     >     PPICLF_XM1B    (PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ,3
     >                    ,PPICLF_LEE)

      REAL                          PPICLF_XERANGE(2,3,PPICLF_LEE)
      COMMON /PPICLF_ELEMENT_RANGE/ PPICLF_XERANGE

      INTEGER PPICLF_LRMAX
      PARAMETER(PPICLF_LRMAX = 6)
      COMMON /PPICLF_PRO_FLD_CI/ PPICLF_ER_MAP,PPICLF_ER_MAPC
     >                        ,PPICLF_ER_MAPS,PPICLF_EL_MAP
     >                        ,PPICLF_MODGP,PPICLF_NELTB,PPICLF_NELTBB
      INTEGER PPICLF_ER_MAP (PPICLF_LRMAX,PPICLF_LEE),
     >        PPICLF_ER_MAPC(PPICLF_LRMAX,PPICLF_LEE),
     >        PPICLF_ER_MAPS(PPICLF_LRMAX,PPICLF_LEE),
     >        PPICLF_MODGP  (PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ
     >                      ,PPICLF_LEE,4),
     >        PPICLF_EL_MAP (8,PPICLF_LEE), PPICLF_NELTB,PPICLF_NELTBB






      integer ppiclf_nee
      common /ppiclf_external_grid_i/ ppiclf_nee

      real ppiclf_gride_x(PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ,PPICLF_LEE),
     >     ppiclf_gride_y(PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ,PPICLF_LEE),
     >     ppiclf_gride_z(PPICLF_LEX,PPICLF_LEY,PPICLF_LEZ,PPICLF_LEE)
      common /ppiclf_external_grid_r/
     >              ppiclf_gride_x,ppiclf_gride_y,ppiclf_gride_z





      INTEGER PPICLF_LIP_GP
      PARAMETER(PPICLF_LIP_GP = 5)
      COMMON /PPICLF_GHOST_PART_I/  PPICLF_IPROP_GP
